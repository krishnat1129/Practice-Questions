class Solution(object):
    def longestCommonPrefix(self, strs):
        if len(strs)<2 : return(strs[0])
        list = sorted(strs)
        LCF = ""
        word1 = list[0]
        word2 = list[-1]
        for i in range(len(word1)) : 
            if word1[i]==word2[i] : 
                LCF = LCF + word1[i]
            else : break
        return LCF
